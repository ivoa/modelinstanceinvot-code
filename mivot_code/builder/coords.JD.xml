<INSTANCE dmrole="" dmtype="coords:JD">
   <!-- Extension of TimeInstant for time expressed in Julian days.  Note that JD does not properly specify a time stamp unless it is related to a time scale and reference position.  Precision can easily become an issue with JD, as the numbers tend to be large." -->
   <!-- The Coordinate system can be pushed up to the GLOBALS and replaced here with a REFERENCE-->
   <INSTANCE dmrole="coords:Coordinate.coordSys" dmtype="coords:TimeSys">
      <!-- Specialized coordinate system for the Temporal domain.  This object SHOULD include an appropriate TimeFrame.  If a CoordSpace is not provided, it is assumed to be represented by a Standard 1-Dimensional Coordinate Space as described in Appendix B." -->
      <INSTANCE dmrole="coords:PhysicalCoordSys.coordSpace" dmtype="coords:PhysicalCoordSpace">
         <!-- Abstract head of coordinate spaces related to physical properties." -->
         <COLLECTION dmrole="coords:CoordSpace.axis">
            <INSTANCE dmrole="" dmtype="coords:Axis">
               <!-- The abstract parent class for all coordinate axis types.  We provide concrete classes for the most common types of data, Continuous, Binned, and Discrete, but allow extension for other types as needed." -->
               <ATTRIBUTE dmrole="coords:Axis.name" dmtype="coords:ivoa:string" unit="" ref="@@@@@" value=""/>
            </INSTANCE>
         </COLLECTION>
      </INSTANCE>
      <INSTANCE dmrole="coords:PhysicalCoordSys.frame" dmtype="coords:TimeFrame">
         <!-- A TimeFrame SHALL include a time scale and reference position.  It MAY also include a reference direction." -->
         <ATTRIBUTE dmrole="coords:TimeFrame.refPosition" dmtype="coords:coords:RefLocation" unit="" ref="@@@@@" value=""/>
         <ATTRIBUTE dmrole="coords:TimeFrame.timescale" dmtype="coords:ivoa:string" unit="" ref="@@@@@" value=""/>
         <ATTRIBUTE dmrole="coords:TimeFrame.refDirection" dmtype="coords:coords:RefLocation" unit="" ref="@@@@@" value=""/>
      </INSTANCE>
   </INSTANCE>
   <ATTRIBUTE dmrole="coords:JD.date" dmtype="coords:ivoa:real" unit="" ref="@@@@@" value=""/>
</INSTANCE>
