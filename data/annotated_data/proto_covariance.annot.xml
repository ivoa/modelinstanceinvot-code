			<!-- Sky position Errors on RA and DEC correlated -->
			<FIELD name="pos_RA" ucd="pos.eq.ra;meta.main" datatype="double" unit="deg" />
			<FIELD name="pos_DEC" ucd="pos.eq.dec;meta.main" datatype="double" unit="deg" />
